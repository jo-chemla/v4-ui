{
  "name": "pooltogether-skeleton-ui",
  "version": "1.0.0",
  "private": true,
  "author": "PoolTogether Inc.",
  "scripts": {
    "reinstall": "rm -rf .next out ./node_modules && yarn cache clean && yarn",
    "dev": "npm-run-all --parallel start:app",
    "start:app": "PORT=3000 node --max-old-space-size=8192 server.js",
    "build": "NODE_OPTIONS=\"--max_old_space_size=16384\" next build && next export && yarn postexport",
    "dist": "yarn build",
    "export": "next export",
    "postexport": "node post-export.js",
    "distrelease": "yarn dist",
    "start": "NODE_ENV=production PORT=80 node server.js",
    "test": "jest",
    "testcov": "jest --coverage --coverageDirectory=coverage && open ./coverage/index.html",
    "watch": "yarn run test --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{ts,js}"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/lib/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "verbose": true
  },
